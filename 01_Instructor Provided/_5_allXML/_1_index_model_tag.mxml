<?xml version="1.0" encoding="utf-8"?>
<mx:Application xmlns:mx="http://www.adobe.com/2006/mxml" 
	layout="absolute" creationComplete="bookHandler(bookStock)">
<mx:Script>
<![CDATA[ private function bookHandler(theBook:Object):void
{		trace(theBook.name);
		trace(theBook.author); } 
]]>
</mx:Script>
<mx:Model id="bookStock">
<stock>
<name>The Da Vinci Code</name>
<author>Not Me</author>
<category>Fiction</category>
<description>Controversial</description>
</stock>
</mx:Model>
</mx:Application> 